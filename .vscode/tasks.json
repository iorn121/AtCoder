{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "runCode",
      "detail": "test my code for calculating samples",
      "type": "shell",
      "command": "/System/Volumes/Data/opt/homebrew/bin/oj",
      "args": [
        // コマンドへ渡す引数
        "t",
        "-c",
        "\"/opt/homebrew/bin/python3 ${fileDirname}/main.py\"",
        "-d",
        "${fileDirname}/tests"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "submitCode",
      "type": "shell",
      "command": "acc",
      "args": ["s", "${fileDirname}/main.py"]
    }
  ]
}
